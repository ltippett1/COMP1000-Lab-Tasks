#include <iostream>
using namespace std;

int main () {

    int num1;

    cout << "Please enter the amount of marks:";
    cin >> num1;

    if (num1 >=90) {
        cout << "Grade A" <<endl;
    }  

    else if (num1 >=80) {
        cout << "Grade B" <<endl;
    }

    else if (num1 >=70) {
        cout << "Grade C" <<endl;
    }

    else if (num1 >=60) {
        cout << "Grade D" <<endl;
    }

    else {
        cout << "Grade F" <<endl;
    }

    return 0;
}
